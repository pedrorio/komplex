{
  "AWSEBDockerrunVersion": 2,
  "containerDefenitions": [
    {
      "name": "client",
      "image": "pedrorio/complex-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "api",
      "image": "pedrorio/complex-server",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "pedrorio/complex-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "pedrorio/complex-nginx",
      "hostname": "nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "client",
        "api"
      ]

    }
  ]
}